# API Keys for accessing the proxy service (comma-separated)
API_KEYS=your-secret-key-1,your-secret-key-2

# OpenRouter API key for backend provider
OPENROUTER_API_KEY=your-openrouter-api-key

# Google Gemini API key for backend provider
GEMINI_API_KEY=your-gemini-api-key

# HTTPS Configuration
# Your domain name (required for SSL certificates)
DOMAIN=your-domain.com

# Email for Let's Encrypt certificate registration
ACME_EMAIL=your-email@example.com

# Optional: Environment (development/production)
# ENVIRONMENT=production

# Optional: Path to the model mapping configuration file
# CONFIG_PATH=config.yml

# Port Configuration
# HTTP port for external access (default: 80)
# HTTP_PORT=80

# HTTPS port for external access (default: 443)
# HTTPS_PORT=443

# Advanced Log Storage System Configuration
# Enable SQLite-based log storage system (default: false)
# LOGDB_ENABLED=true

# Enable full-text search (default: false)
# LOGDB_FTS_ENABLED=true

# Enable dialog grouping (default: false)
# LOGDB_GROUPING_ENABLED=true

# Partition granularity: daily|weekly (default: daily)
# LOGDB_PARTITION_GRANULARITY=daily

# Include raw logs in bundles (default: false)
# LOGDB_BUNDLE_INCLUDE_RAW=false

# Concurrent file processing (default: 2)
# LOGDB_IMPORT_PARALLELISM=2

# Memory limit for processing in MB (default: 256)
# LOGDB_MEMORY_CAP_MB=256 

# =====================
# Logs UI API (ai_proxy_ui)
# =====================

# Comma-separated list of API keys for Logs UI API (user access)
# LOGUI_API_KEYS=logs-ui-user-key-1,logs-ui-user-key-2

# Comma-separated list of admin API keys for Logs UI API (admin access)
# LOGUI_ADMIN_API_KEYS=logs-ui-admin-key-1

# Allowed CORS origins for Logs UI API
# Example: https://logs.your-domain.com,http://localhost:5173
# LOGUI_ALLOWED_ORIGINS=*

# Enable optional raw text logs features in UI API
# LOGUI_ENABLE_TEXT_LOGS=false

# Root directory for SQLite partitions (read-only access)
# LOGUI_DB_ROOT=./logs/db